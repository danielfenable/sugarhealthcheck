<div class="dashlet-options">
    <div class="row-fluid">
        <div class="span8">
            {{#each this.meta.filter_duration}}
                {{field ../this model=../this.model template="edit"}}
            {{/each}}
        </div>
        <div class="span4">
            <div data-toggle="buttons-radio" class="btn-group pull-right dashlet-group">
                <button class="btn interactions-chart {{#eq params.list "my"}}active{{/eq}}" value="my"><i class="fa fa-user"></i></button>
                <button class="btn interactions-chart {{#eq params.list "all"}}active{{/eq}}" value="all"><i class="fa fa-users"></i></button>
            </div>
        </div>
    </div>
</div>
<div>
    <svg></svg>
</div>
