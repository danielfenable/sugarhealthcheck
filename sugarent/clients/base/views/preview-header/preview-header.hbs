<h1>{{str "LBL_PREVIEW"}}</h1>
<div class="btn-toolbar pull-right">
    <div class="btn-group">
        {{#unless layout.hideNextPrevious}}
        <a class="btn btn-invisible{{#unless layout.previous}} disabled{{/unless}}" data-direction="left"><i class="fa fa-chevron-left"></i></a>
        <a class="btn btn-invisible{{#unless layout.next}} disabled{{/unless}}" data-direction="right"><i class="fa fa-chevron-right"></i></a>
        {{/unless}}
    </div>
    <div class="btn-group">
        <a class="btn btn-invisible closeSubdetail" data-toggle="tab"><i class="fa fa-times"></i></a>
    </div>
</div>
