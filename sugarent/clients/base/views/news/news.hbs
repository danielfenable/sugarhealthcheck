{{#if responseData.results}}
{{#each responseData.results}}
    <div class="news-article">
        <h5><a href="{{this.unescapedUrl}}">{{{this.titleNoFormatting}}}</a>
        </h5> ({{{this.publisher}}})
        <p>{{{this.content}}}</p>
    </div>
{{/each}}
{{else}}
    <div class="block-footer">{{str "LBL_NO_DATA_AVAILABLE"}}</div>
{{/if}}
