<div class="search-filter">
    <div class="control-group">
        <div class="filter controls controls-two btn-group-fit">
            <div class="toggle-actions actions">
                <div class="btn-group" data-toggle="buttons-radio">
                    {{#each toggles}}
                        <a type="button" class="btn {{#eq ../options.meta.default toggle}}active{{/eq}}" data-view="{{toggle}}" data-mode="view" rel="tooltip" title="{{str title}}"><i class="fa {{class}}"></i></a>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>
<div class="main-content"></div>
