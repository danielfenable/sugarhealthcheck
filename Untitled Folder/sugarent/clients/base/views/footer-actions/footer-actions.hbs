{{#if isAuthenticated}}
   {{#if isShortcutsEnabled}}<button data-action="shortcuts" class="btn btn-invisible" aria-label="{{str "LBL_SHORTCUTS"}}" role="link"><i class="fa fa-keyboard-o"></i><span class="action-label">  {{str "LBL_SHORTCUTS"}}</span></button>{{/if}}
   <button data-action="tour" class="btn btn-invisible" aria-label="{{str "LNK_TOUR"}}" role="link"><i class="fa fa-road"></i><span class="action-label"> {{str "LNK_TOUR"}}</span></button>
   <button data-action="feedback" class="btn btn-invisible" aria-label="{{str "LBL_FEEDBACK"}}" role="link"><i class="fa fa-comment"></i><span class="action-label"> {{str "LBL_FEEDBACK"}}</span></button>
   <button data-action="help" class="btn btn-invisible disabled" aria-label="{{str "LBL_HELP"}}" role="link"><i class="fa fa-exclamation-circle"></i><span class="action-label"> {{str "LBL_HELP"}}</span></button>
   <button data-action="support" class="btn btn-invisible" aria-label="{{str "LBL_SUPPORT"}}" role="link"><i class="fa fa-question-circle"></i><span class="action-label"> {{str "LBL_SUPPORT"}}</span></button>
{{/if}}
