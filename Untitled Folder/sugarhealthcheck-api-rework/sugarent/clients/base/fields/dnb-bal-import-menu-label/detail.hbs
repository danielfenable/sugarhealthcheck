{{#if value}}
    {{#with def}}
    <li>
        <a class="ellipsis_inline" tabindex="-1" data-navbar-menu-item="{{label}}"
            {{#if event}}
                href="javascript:void(0);" data-event="{{event}}" track="click:{{event}}"
            {{else}}
               href="{{route}}" data-route="{{route}}"
               {{#if openwindow}}
                   data-openwindow="true"
               {{/if}}
               track="click:{{route}}"
            {{/if}}>
            {{#if icon}}
                <i class="fa {{icon}}"></i>
            {{/if}}
            {{../value}}
        </a>
    </li>
    {{/with}}
{{/if}}
