{{#each resources}}
<li class="resource {{css_class}}">
    <a href="{{url}}" target="_blank"><span class="label label-module label-module-md pull-left {{color}}"><i class="fa fa-2x {{icon}}"></i></span></a>
    <div class="resource-info">
        <a href="{{url}}" target="_blank">{{str link module}}</a>
        <p class="details">{{str teaser module}}</p>
    </div>
</li>
{{/each}}
