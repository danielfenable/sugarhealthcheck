<div data-content="chart" class="dnb-charts">
    {{#if contractInfo}}
        <div class="block-footer">
            {{contractInfo.cntLabel}}&nbsp;:&nbsp;{{contractInfo.cntInfo}}
        </div>
    {{/if}}
    {{#each dnbChartIds}}
        <div class="nv-chart nv-chart-donut dnb-meter hide" id="dnb{{this}}">
            <svg id="{{this}}"></svg>
        </div>
    {{/each}}
</div>
<div class="block-footer hide" data-content="nodata">{{str "LBL_DNB_METER_LOADING"}}...</div>
